<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.shangbb.studysample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".SysApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CustomViewActivity"
            android:label="@string/title_activity_custom_view">
        </activity>
        <activity android:name=".activity.CanvasDemoActivity">
        </activity>
    </application>

</manifest>